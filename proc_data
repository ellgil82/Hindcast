## Process SMB stream: pd
# Process evaporation
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_evaporation_rate.nc -f 0

# Process sublimation
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_sublimation_rate.nc -f 1

# Process land mean potential evaporation
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_potential_evaporation_rate.nc -f 2

# Process snow melt amnt
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_land_snow_melt_amnt.nc -f 3

# Process snow melt flux
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_land_snow_melt_flux.nc -f 4

# Process snow melt rate
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_land_snow_melt_rate.nc -f 5

# Process surface runoff rate
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_surface_runoff_rate.nc -f 6

# Process subsurface runoff rate
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_subsurface_runoff_rate.nc -f 7

# Process tiled snow mass balance
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_tiled_snow_mass_balance.nc -f 8

# Process tiled snow melt rate
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_tiled_snow_melt_rate.nc -f 9

# Process tiled snow refreeze rate
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_tiled_snow_refreeze_rate.nc -f 10

# Process tiled snow solid mass change
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_tiled_snow_solid_mass_change.nc -f 11

# Process tiled snow liquid mass change
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_tiled_snow_liquid_mass_change.nc -f 12

# Process tiled snow net runoff
/users/toil/bin/conv2nc.tcl -i /data/mac/ellgil82/hindcast/output/2008/*hindcast*_pd000.pp -o /data/mac/ellgil82/hindcast/output/2008_tiled_snow_net_runoff.nc -f 13

